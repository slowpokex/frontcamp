{
  "name": "frontcamp_task",
  "version": "1.0.0",
  "description": "Frontcamp 3 tasks",
  "scripts": {
    "test": "mocha --require babel-core/register --reporter spec",
    "test:watch": "mocha --require babel-core/register --watch",
    "start": "babel-node index.js",
    "watch": "nodemon index.js --exec babel-node",
    "build": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "precommit": "npm run build:prod && git add docs/**",
    "prepush": "npm test"
  },
  "author": "Mikita Isakau",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^4.0.0-beta.2",
    "express": "^4.16.2",
    "pug": "^2.0.0-rc.4",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "helmet": "^3.10.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jasmine": "^2.8.0",
    "mocha": "^4.1.0",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
